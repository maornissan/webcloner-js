{
  "name": "webcloner-js",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "webcloner-js": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:electron": "tsc -p tsconfig.electron.json && node copy-electron-assets.mjs",
    "start": "node dist/cli.js",
    "start:electron": "npm run build:electron && electron dist-electron/electron/main.cjs --no-sandbox",
    "dev": "npm run build && node dist/cli.js",
    "dev:watch": "ts-node --esm src/cli.ts",
    "dev:electron": "NODE_ENV=development npm run build:electron && electron dist-electron/electron/main.cjs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "web",
    "scraper",
    "cloner",
    "downloader",
    "offline"
  ],
  "author": "maornisan",
  "license": "ISC",
  "description": "Advanced stealthy website cloner with proxy support",
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.1.0",
    "https-proxy-agent": "^7.0.2",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@types/mime-types": "^2.1.4",
    "@types/node": "^24.9.2",
    "electron": "^39.0.0",
    "eslint": "^9.39.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
